{
  "name": "demo-app",
  "version": "0.40.3",
  "devDependencies": {
    "app-framework": "^0.40.3"
  },
  "scripts": {
    "dev": "cd node_modules/app-framework && node scripts/dev-server.js",
    "patch": "npm version patch && cd node_modules/app-framework && node scripts/build.js",
    "minor": "npm version minor && cd node_modules/app-framework && node scripts/build.js",
    "major": "npm version major && cd node_modules/app-framework && node scripts/build.js",
    "deploy": "node node_modules/app-framework/scripts/prepareFirebaseDeploy.js && firebase login && firebase deploy && node node_modules/app-framework/scripts/cleanupFirebaseDeploy.js",
    "check": "standard >standard-check.log \"app.vue\" \"pages/*.vue\" --plugin html",
    "fix": "standard >standard-check.log \"app.vue\" \"pages/*.vue\" --plugin html --fix"
  },
  "title": "Demo App",
  "theme": "ios",
  "defaultLanguage": "en",
  "showPhoneFrameOnDesktop": true,
  "routes": {
    "home": "home",
    "panelLeft": "panelLeft",
    "panelRight": "panelRight",
    "loginScreen": "loginScreen",
    "popup": "popup",
    "form": "form",
    "image": "image",
    "simpleTodo": "simpleTodo",
    "firebasePublic": "firebasePublic",
    "firebasePrivate": "firebasePrivate",
    "multipleLanguages": "multipleLanguages",
    "simplePage": "simplePage",
    "tabs": "tabs",
    "tabs-sub": "tabs-sub",
    "flexibleRouting/blog/:blogId/post/:postId": "flexibleRouting",
    "login": "login"
  },
  "loadIconFonts": {
    "framework7": false,
    "material": false,
    "ion": false,
    "fontawesome": true
  },
  "faviconIcon": "images/faviconIcon.png",
  "faviconBackgroundColor": "#ffffff",
  "firebase": {
    "apiKey": "AIzaSyAvzTiqd9fKR-h47Uxl4iXwqSMU1VjGdII",
    "authDomain": "app-framework-9045a.firebaseapp.com",
    "databaseURL": "https://app-framework-9045a.firebaseio.com",
    "storageBucket": "app-framework-9045a.appspot.com",
    "messagingSenderId": "690341427128",
    "useDatabaseService": true,
    "useStorageService": false,
    "useHostingService": true,
    "useDatabaseRules": true,
    "userRegistration": true
  },
  "loginRequired": [
    "privateFirebase"
  ],
  "buildSourcemaps": false
}
